{
  "root_dir": "/home/kangle/dataset/PedBicCarData",
  "sample_rate": {
    "sample_rate_t": 2,
    "sample_rate_f": 2
  },
  "classifier": {
    "method": "nn",
    "parameter": {
      "optimizer": "Adam",
      "learning_rate_schedule": "piecewise",
      "learning_rate_policy": {
        "base_lr": 3e-4
        "step_size": 10,
        "decay_rate": 2
      },
      "learning_rate": 0.001,
      "loss": "categorical_crossentropy",
      "metrics": "accuracy",
      "batch_size": 128,
      "epochs": 5
    },
    "model": [
      {
        "name": "layer_01",
        "type": "Conv2D",
        "filter_height": 3,
        "filter_width": 3,
        "channel": 32,
        "initializer": "he_norm",
        "activation": "relu",
        "padding": "same",
        "regularizer": "None"
      },
      {
        "name": "layer_02",
        "type": "MaxPooling2D"
      },
      {
        "name": "layer_03",
        "type": "Conv2D",
        "filter_height": 3,
        "filter_width": 3,
        "channel": 64,
        "initializer": "he_norm",
        "activation": "relu",
        "padding": "same",
        "regularizer": "None"
      },
      {
        "name": "layer_04",
        "type": "MaxPooling2D"
      },
      {
        "name": "layer_05",
        "type": "Conv2D",
        "filter_height": 3,
        "filter_width": 3,
        "channel": 128,
        "initializer": "he_norm",
        "activation": "relu",
        "padding": "same",
        "regularizer": "None"
      },
      {
        "name": "layer_06",
        "type": "MaxPooling2D"
      },
      {
        "name": "layer_07",
        "type": "Conv2D",
        "filter_height": 3,
        "filter_width": 3,
        "channel": 256,
        "initializer": "he_norm",
        "activation": "relu",
        "padding": "same",
        "regularizer": "None"
      },
      {
        "name": "layer_08",
        "type": "MaxPooling2D"
      },
      {
        "name": "layer_09",
        "type": "Conv2D",
        "filter_height": 3,
        "filter_width": 3,
        "channel": 256,
        "initializer": "he_norm",
        "activation": "relu",
        "padding": "same",
        "regularizer": "None"
      },
      {
        "name": "layer_10",
        "type": "AveragePooling2D"
      },
      {
        "name": "layer_11",
        "type": "Flatten"
      },
      {
        "name": "layer_12",
        "type": "Dense",
        "units": 256,
        "activation": "relu"
      },
      {
        "name": "layer_13",
        "type": "Dropout",
        "rate": 0.2
      },
      {
        "name": "layer_14",
        "type": "Dense",
        "units": 5,
        "activation": "softmax"
      }
    ]
  }
}
